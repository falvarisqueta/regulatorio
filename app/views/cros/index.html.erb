<p id="notice"><%= notice %></p>

<h1>Cros</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cros.each do |cro| %>
      <tr>
        <td><%= link_to 'Show', cro %></td>
        <td><%= link_to 'Edit', edit_cro_path(cro) %></td>
        <td><%= link_to 'Destroy', cro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cro', new_cro_path %>
